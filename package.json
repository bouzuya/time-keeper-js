{
  "name": "time-keeper",
  "description": "TimeKeeper.js : A simple date-time library",
  "version": "1.0.0",
  "author": {
    "name": "bouzuya",
    "email": "m@bouzuya.net",
    "url": "http://bouzuya.net/"
  },
  "beater": {
    "dir": "./.tmp/test/",
    "require": [
      "./scripts/enable-power-assert.js"
    ]
  },
  "bugs": {
    "url": "https://github.com/bouzuya/time-keeper-js/issues"
  },
  "devDependencies": {
    "@types/power-assert": "1.4.29",
    "@types/proxyquire": "1.3.27",
    "@types/sinon": "1.16.32",
    "beater": "0.4.1",
    "beater-cli": "0.2.4",
    "cpx": "1.5.0",
    "espower-loader": "1.2.0",
    "npm-run-all": "3.1.1",
    "power-assert": "1.4.2",
    "proxyquire": "1.7.10",
    "rimraf": "2.5.4",
    "sinon": "1.17.6",
    "typescript": "2.0.10",
    "watch": "1.0.1"
  },
  "files": [
    "_",
    "types",
    "get.d.ts",
    "get.js",
    "in-time-zone.d.ts",
    "in-time-zone.js",
    "index.d.ts",
    "index.js",
    "is-leap-year.d.ts",
    "is-leap-year.js",
    "now.d.ts",
    "now.js",
    "parse-iso-string.d.ts",
    "parse-iso-string.js",
    "parse-unix-time.d.ts",
    "parse-unix-time.js",
    "plus.d.ts",
    "plus.js",
    "to-iso-string.d.ts",
    "to-iso-string.js",
    "to-time-zone-offset-string.d.ts",
    "to-time-zone-offset-string.js",
    "to-unix-time.d.ts",
    "to-unix-time.js"
  ],
  "homepage": "https://github.com/bouzuya/time-keeper-js#readme",
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bouzuya/time-keeper-js.git"
  },
  "scripts": {
    "build": "npm-run-all -s 'build:ts' 'build:copy'",
    "build:copy": "cpx '.tmp/src/**/*.{d.ts,js}' './'",
    "build:ts": "tsc",
    "clean": "rimraf '.tmp' '_' 'types' '*.js' '*.d.ts'",
    "prepublish": "npm-run-all -s 'clean' 'build'",
    "test": "beater",
    "watch": "npm-run-all -p 'watch:*'",
    "watch:copy": "watch --wait 2 'npm run copy' '.tmp/src/'",
    "watch:test": "watch --wait 2 'npm test' '.tmp/test/'",
    "watch:ts": "tsc -- --watch"
  },
  "typings": "index.d.ts"
}
